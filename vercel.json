{
    "version": 2,
    "builds": [
      {
        "src": "backend.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      },
      {
        "src": "/api/(.*)",
        "dest": "/backend.js"
      }
    ]
  }